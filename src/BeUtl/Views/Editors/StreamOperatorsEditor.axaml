<UserControl
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:vm="using:BeUtl.ViewModels.Editors"
    xmlns:local="using:BeUtl.Views.Editors"
    xmlns:ui="using:FluentAvalonia.UI.Controls"
    mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
    DragDrop.AllowDrop="True"
    x:DataType="vm:StreamOperatorsEditorViewModel"
    x:Class="BeUtl.Views.Editors.StreamOperatorsEditor">
    <ScrollViewer x:Name="scrollViewer">
        <ItemsControl Items="{CompiledBinding Items}" x:Name="itemsControl">
            <ItemsControl.ItemTemplate>
                <DataTemplate>
                    <local:StylingOperatorEditor />
                </DataTemplate>
            </ItemsControl.ItemTemplate>
        </ItemsControl>
    </ScrollViewer>
</UserControl>
