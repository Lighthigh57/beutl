<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:BEditorNext"
             xmlns:convert="using:BEditorNext.Converters"
             xmlns:aconverters="using:Avalonia.Markup.Xaml.Converters"
             x:Class="BEditorNext.App">
    <Application.Resources>
        <convert:PixelSizeConverter x:Key="PixelSizeConverter" />
        <aconverters:ColorToBrushConverter x:Key="ColorToBrushConverter" />
    </Application.Resources>
    <Application.Styles>
        <StyleInclude Source="avares://BEditorNext.Controls/Styles.axaml" />
        <StyleInclude Source="avares://BEditorNext.Language/Strings.axaml" />
        <StyleInclude Source="avares://BEditorNext.Language/Strings_ja-JP.axaml" />
    </Application.Styles>
</Application>
