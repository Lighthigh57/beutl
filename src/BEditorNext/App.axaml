<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:BEditorNext"
             xmlns:convert="using:BEditorNext.Converters"
             x:Class="BEditorNext.App">
    <Application.Resources>
        <convert:PixelSizeConverter x:Key="PixelSizeConverter" />
    </Application.Resources>
    <Application.Styles>
        <FluentTheme Mode="Light" />

        <StyleInclude Source="avares://BEditorNext.Controls/Styles.axaml" />
        <StyleInclude Source="avares://BEditorNext.Language/Strings.axaml" />
        <StyleInclude Source="avares://BEditorNext.Language.Japanese/Strings.axaml" />
    </Application.Styles>
</Application>
