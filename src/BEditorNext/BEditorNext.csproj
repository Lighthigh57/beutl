<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <RuntimeIdentifiers>win-x64;linux-x64;osx-x64</RuntimeIdentifiers>
  </PropertyGroup>
  <ItemGroup>
    <AvaloniaResource Include="Assets\**" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Roslynator.Analyzers" Version="3.3.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="XamlNameReferenceGenerator" Version="1.3.4" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\BEditorNext.Controls\BEditorNext.Controls.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Views\Editors\NumberEditor.axaml.cs">
      <DependentUpon>NumberEditor.axaml</DependentUpon>
    </Compile>
  </ItemGroup>
  <Import Project="$(RootDirectory)\build\props\Avalonia.props" />
  <Import Project="$(RootDirectory)\build\props\ReactiveProperty.props" />
  <Import Project="$(RootDirectory)\build\props\Microsoft.Extensions.DependencyInjection.props" />
  <Import Project="$(RootDirectory)\build\props\CoreLibraries.props" />
</Project>
