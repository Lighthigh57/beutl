<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style Selector="Border.CardStyle">
        <Setter Property="Background" Value="{DynamicResource CardBackgroundFillColorDefaultBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource CardStrokeColorDefaultBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="{DynamicResource ControlCornerRadius}" />
    </Style>

    <Style Selector="PathIcon.Small">
        <Setter Property="FontSize" Value="14" />
        <Setter Property="Width" Value="14" />
        <Setter Property="Height" Value="14" />
    </Style>

    <StyleInclude Source="avares://BeUtl.Controls/Styling/BaseResources.axaml" />
    <StyleInclude Source="avares://BeUtl.Controls/Styling/FluentIconsFilled.axaml" />
    <StyleInclude Source="avares://BeUtl.Controls/Styling/FluentIconsRegular.axaml" />
    <StyleInclude Source="avares://BeUtl.Controls/Styling/Player.axaml" />
    <StyleInclude Source="avares://BeUtl.Controls/Styling/NavigationView.axaml" />
    <StyleInclude Source="avares://BeUtl.Controls/Styling/DraggableTabItem.axaml" />
    <StyleInclude Source="avares://BeUtl.Controls/Styling/DraggableTabView.axaml" />
</Styles>
